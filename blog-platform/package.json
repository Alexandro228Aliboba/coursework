		"express": "*",
		"express-ejs-layouts": "^2.5.1",
		"express-session": "*",
		"helmet": "*",
		"multer": "*",
		"mysql2": "*",
		"passport": "*",
		"passport-local": "*",
		"quill": "*",
		"selfsigned": "*",
		"sequelize": "*"
	},
	"devDependencies": {
		"nodemon": "*",
		"sass": "*"
	}
}
